    "@types/xml2js": "^0.4.11",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/jest": "^29.1.2",
    "@types/node-forge": "^1.3.0",
    "axios": "^1.1.2",
    "easy-soap-request": "^5.1.0",
    "jest": "^29.1.2",
    "jest-each": "^29.1.2",
